# üåê SHADOW.AI II - DEVELOPMENT ENVIRONMENT
# Configuration for local development and testing

# Environment
NODE_ENV=development
LOG_LEVEL=debug
SANDBOX_MODE=true

# Database Configuration
POSTGRES_DB=shadow_ai_dev
POSTGRES_USER=dev_user
POSTGRES_PASSWORD=dev_password
POSTGRES_URL=postgresql://dev_user:dev_password@localhost:5432/shadow_ai_dev

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Authentication
NEXTAUTH_SECRET=dev_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# Exchange API Keys (Sandbox/Testnet)
BINANCEUS_KEY=dev_binance_us_key
BINANCEUS_SECRET=dev_binance_us_secret

KRAKEN_KEY=dev_kraken_key
KRAKEN_SECRET=dev_kraken_secret

OKX_KEY=dev_okx_key
OKX_SECRET=dev_okx_secret
OKX_PASSPHRASE=dev_okx_passphrase

# Monitoring
GRAFANA_PASSWORD=dev_admin

# Security
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# Performance
MAX_MEMORY=128M
CPU_LIMIT=0.1




