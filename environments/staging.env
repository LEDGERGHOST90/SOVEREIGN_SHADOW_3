# üåê SHADOW.AI II - STAGING ENVIRONMENT
# Configuration for staging/testing deployment

# Environment
NODE_ENV=staging
LOG_LEVEL=debug
SANDBOX_MODE=true

# Database Configuration
POSTGRES_DB=shadow_ai_staging
POSTGRES_USER=trader
POSTGRES_PASSWORD=staging_password
POSTGRES_URL=postgresql://trader:staging_password@postgres:5432/shadow_ai_staging

# Redis Configuration
REDIS_URL=redis://redis:6379

# Authentication
NEXTAUTH_SECRET=staging_nextauth_secret
NEXTAUTH_URL=http://localhost

# Exchange API Keys (Sandbox/Testnet)
BINANCEUS_KEY=test_binance_us_key
BINANCEUS_SECRET=test_binance_us_secret

KRAKEN_KEY=test_kraken_key
KRAKEN_SECRET=test_kraken_secret

OKX_KEY=test_okx_key
OKX_SECRET=test_okx_secret
OKX_PASSPHRASE=test_okx_passphrase

# Monitoring
GRAFANA_PASSWORD=staging_admin

# Security
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# Performance
MAX_MEMORY=256M
CPU_LIMIT=0.25
