<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.sovereignshadow.state-updater</string>

    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Volumes/LegacySafe/SovereignShadow_II/bin/update_state_hourly.sh</string>
    </array>

    <key>StartInterval</key>
    <integer>3600</integer>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/Volumes/LegacySafe/SovereignShadow_II/logs/state_updater_stdout.log</string>

    <key>StandardErrorPath</key>
    <string>/Volumes/LegacySafe/SovereignShadow_II/logs/state_updater_stderr.log</string>

    <key>WorkingDirectory</key>
    <string>/Volumes/LegacySafe/SovereignShadow_II</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>
</dict>
</plist>
